{
  "workbench.colorTheme": "Atom One Light",
  "workbench.startupEditor": "none",
  "editor.fontFamily": "Inconsolata, Consolas, 'Courier New', Courier",
  "editor.lineHeight": 20,
  "editor.renderWhitespace": "boundary",
  "editor.multiCursorModifier": "alt",
  "editor.wordWrap": "on",
  "editor.cursorBlinking": "blink",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "files.associations": {
    "*.test.ts.snap": "javascript",
    "*.spec.ts.snap": "javascript",
    "*.tpl": "php"
  },
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.confirmOnExit": true,
  "telemetry.enableTelemetry": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "debug.allowBreakpointsEverywhere": true,
  "window.titleBarStyle": "custom",
  "breadcrumbs.enabled": true,
  "window.zoomLevel": 0,
  "git.autofetch": true,
  "workbench.statusBar.visible": true,
  "git.confirmSync": false,
  "editor.cursorSmoothCaretAnimation": true,
  "terminal.integrated.rendererType": "canvas",
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "terminal.integrated.shell.linux": "fish",
  "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7-preview\\pwsh.exe",
  "terminal.integrated.automationShell.windows": "C:\\Program Files\\PowerShell\\7-preview\\pwsh.exe",
  "workbench.activityBar.visible": true,
  "markdown.preview.scrollEditorWithPreview": false,
  "editor.minimap.enabled": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.renderSideBySide": true,
  "editor.cursorSurroundingLines": 5,
  "scm.defaultViewMode": "tree",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "importCost.bundleSizeDecoration": "gzipped",
  "importCost.mediumPackageColor": "#788ca1",
  "importCost.smallPackageColor": "#6a737d",
  "editor.snippetSuggestions": "bottom",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "extensions.ignoreRecommendations": false,
  "workbench.sideBar.location": "left",
  "liveshare.featureSet": "insiders",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "peacock.favoriteColors": [

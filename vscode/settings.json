{
  "workbench.startupEditor": "none",
  "editor.renderWhitespace": "boundary",
  "editor.multiCursorModifier": "alt",
  "editor.wordWrap": "on",
  "editor.cursorBlinking": "blink",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "files.associations": {
    "*.test.ts.snap": "javascript",
    "*.spec.ts.snap": "javascript",
    "*.tpl": "php"
  },
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.confirmOnExit": true,
  "telemetry.enableTelemetry": false,
  "debug.allowBreakpointsEverywhere": true,
  "window.titleBarStyle": "custom",
  "breadcrumbs.enabled": true,
  "git.autofetch": true,
  "workbench.statusBar.visible": true,
  "git.confirmSync": false,
  "editor.cursorSmoothCaretAnimation": true,
  "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7-preview\\pwsh.exe",
  "terminal.integrated.automationShell.windows": "C:\\Program Files\\PowerShell\\7-preview\\pwsh.exe",
  "markdown.preview.scrollEditorWithPreview": false,
  "editor.minimap.enabled": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.cursorSurroundingLines": 5,
  "scm.defaultViewMode": "tree",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.snippetSuggestions": "bottom",
  "extensions.ignoreRecommendations": false,
  "liveshare.featureSet": "insiders",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "prettier.requireConfig": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "search.exclude": {
    "**/node_modules": false
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "editor.fontFamily": "'Comic Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.lineHeight": 19,
  "yaml.schemas": {
    "file:///Users/tomsherman/.vscode/extensions/atlassian.atlascode-2.10.12/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "atlascode.jira.enabled": true,
  "atlascode.bitbucket.enabled": false,
  "atlascode.jira.jqlList": [
    {
      "id": "0527c005-5d72-4368-99a2-333ffa967551",
      "enabled": true,
      "name": "My ovotech Issues",
      "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
      "siteId": "3be88982-f9c1-48b3-9e95-8ed8275d1f50",
      "monitor": true
    }
  ],
  "redhat.telemetry.enabled": false,
  "githubPullRequests.fileListLayout": "tree",
  "workbench.sideBar.location": "right",
  "terraform.languageServer": {
    "external": true,
    "pathToBinary": "",
    "args": [
      "serve"
    ],
    "maxNumberOfProblems": 100,
    "trace.server": "off"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jest.autoRun": "off",
  "jest.enableCodeLens": false,
  "workbench.colorTheme": "GitHub Light",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false
